{
  "canisters": {
    "validify_backend": {
      "build": "npx azle validify_backend",
      "candid": "src/validify_backend/validify_backend.did",
      "gzip": true,
      "main": "src/validify_backend/src/index.ts",
      "tech_stack": {
        "cdk": {
          "azle": {}
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      },
      "type": "custom",
      "wasm": ".azle/validify_backend/validify_backend.wasm"
    },
    "validify_frontend": {
      "dependencies": [
        "validify_backend"
      ],
      "source": [
        "src/validify_frontend/dist"
      ],
      "type": "assets",
      "workspace": "validify_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}